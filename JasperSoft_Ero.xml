    <parameter name="P_WOFH_ID" class="java.lang.Integer"/>
    <queryString language="SQL">
        <![CDATA[SELECT 
            hdr.WO_NUMBER,
            hdr.STATUS,
            hdr.PN,
            hdr.SERIAL_NUMBER,
            hdr.CMM AS HDR_CMM,
            (SELECT COMPANY_NAME 
             FROM QCTL.COMPANIES@prod.gatelesis.com cmp
             WHERE cmp.CMP_AUTO_KEY = hdr.CMP_AUTO_KEY 
            ) AS COMPANY_NAME,
            (SELECT DESCRIPTION 
             FROM QCTL.PARTS_MASTER@prod.gatelesis.com pm
             WHERE pm.pn = hdr.pn)
             AS DESCRIPTION, 
            hdr.DTD AS HDR_DTD,
            hdr.M10_PRINTED,
            det.FINDINGS,
            det.REPAIR_DISPOSITION,
            det.MECHANNIC,
            det.MECHANNIC_DATE,
            det.INSPECTOR,
            det.INSPECTOR_DATE,
            det.ITEM_NUMBER,
            det.CMM AS DET_CMM,
            det.CMM_REVISION,
            det.DAMAGE_DIMENSION_W,
            det.DAMAGE_LOCATION,
            det.EAR_REQUEST,
            det.WO_NUMBER AS DET_WO_NUMBER,
            det.ENGINEER,
            det.ENGINEER_DATE,
            det.DAMAGE_SHAPE,
            det.DAMAGE_DIMENSION_H,
            det.DAMAGE_PART_DESCRIPTION
        FROM 
            CRG_WO_FINDINGS_HEADER hdr
        JOIN 
            CRG_WO_FINDINGS_DETAIL det ON hdr.WOFH_ID = det.WOFH_ID
        WHERE 
            hdr.WOFH_ID = $P{P_WOFH_ID}]]>
    </queryString>
